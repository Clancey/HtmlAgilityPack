<Window x:Class="HAPExplorer.UrlDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Open Url" Height="113" Width="403" ResizeMode="NoResize">
    <Grid>
        <TextBox Margin="48,12,37,0" Name="textBox1" Height="23" VerticalAlignment="Top" />
        <Label HorizontalAlignment="Left" Margin="12,12,0,9" Name="label1" Width="30">Url:</Label>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,37,11" Name="btnOpen" VerticalAlignment="Bottom" Width="75" Click="btnOpen_Click" IsDefault="True">_Open</Button>
        <Button Height="23" Margin="188,0,118,11" Name="btnCancel" VerticalAlignment="Bottom" Click="btnCancel_Click" IsCancel="True">_Cancel</Button>
    </Grid>
</Window>
